<?xml version="1.0" encoding="UTF-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android"
    android:insetLeft="0.0px"
    android:insetRight="0.0px" >

    <selector>
        <item android:state_pressed="true">
            <shape>
                <gradient
                    android:angle="270.0"
                    android:endColor="#a0e0b071"
                    android:startColor="#a0a67637" />

                <corners
                    android:bottomLeftRadius="3dip"
                    android:bottomRightRadius="3dip"
                    android:radius="3.0dip"
                    android:topLeftRadius="3.0dip"
                    android:topRightRadius="3.0dip" />

                <stroke
                    android:width="1dp"
                    android:color="#5c3708" />
            </shape>
        </item>
        <item>
            <shape>
                <gradient
                    android:angle="270.0"
                    android:endColor="@color/text_orange"
                    android:startColor="@color/text_orange" />

                <corners
                    android:bottomLeftRadius="3.0dip"
                    android:bottomRightRadius="3.0dip"
                    android:radius="3.0dip"
                    android:topLeftRadius="3.0dip"
                    android:topRightRadius="3.0dip" />

                <stroke
                    android:width="0dp"
                    android:color="@color/text_orange" />
            </shape>
        </item>
    </selector>

</inset>